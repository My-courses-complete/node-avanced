{
  "name": "platziverse-web",
  "version": "1.0.0",
  "description": "A simple IoT Platform - Web Dashboard",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "prestart": "npm run build",
    "start-dev": "DEBUG=platziverse:* nodemon server.js",
    "prestart-dev": "npm run build-dev",
    "build-dev": "browserify -g envify -t babelify -t vueify client/app.js -o public/bundle.js",
    "build": "NODE_ENV=production browserify -g envify -t babelify -t vueify client/app.js | npx uglify-js -c -m -o public/bundle.js",
    "lint": "standard"
  },
  "repository": {
    "type": "git",
    "url": "https//github.com/Platzi-courses-completed/node-avanced"
  },
  "author": "JMR-Mejia",
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^17.0.0",
    "envify": "^4.1.0",
    "nodemon": "^2.0.13",
    "standard": "^16.0.4",
    "uglify-js": "^3.14.2",
    "vueify": "^9.4.1"
  },
  "dependencies": {
    "axios": "^0.22.0",
    "chalk": "^4.1.2",
    "chart.js": "^3.5.1",
    "debug": "^3.2.7",
    "express": "^4.17.1",
    "moment": "^2.29.1",
    "platziverse-agent": "file:../platziverse-agent",
    "random-material-color": "^1.0.5",
    "socket.io": "^4.2.0",
    "socket.io-client": "^4.2.0",
    "vue": "^2.6.14",
    "vue-chartjs": "^3.5.1"
  }
}
